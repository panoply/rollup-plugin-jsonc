#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/bin/acorn/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/acorn@6.4.1/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules:/Users/Panoply/Sites/rollup-plugins/node_modules:/Users/Panoply/Sites/node_modules:/Users/Panoply/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@6.4.1/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../acorn@6.4.1/node_modules/acorn/bin/acorn" "$@"
fi
