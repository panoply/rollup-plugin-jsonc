#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/terser@4.7.0/node_modules/terser/bin/terser/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/terser@4.7.0/node_modules/terser/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/terser@4.7.0/node_modules/terser/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/terser@4.7.0/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules:/Users/Panoply/Sites/rollup-plugins/node_modules:/Users/Panoply/Sites/node_modules:/Users/Panoply/node_modules:/Users/node_modules:/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/terser/bin/terser/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/terser/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else 
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
