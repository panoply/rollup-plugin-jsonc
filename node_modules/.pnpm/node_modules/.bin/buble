#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/buble@0.20.0/node_modules/buble/bin/buble/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/buble@0.20.0/node_modules/buble/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/buble@0.20.0/node_modules/buble/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/buble@0.20.0/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules:/Users/Panoply/Sites/rollup-plugins/node_modules:/Users/Panoply/Sites/node_modules:/Users/Panoply/node_modules:/Users/node_modules:/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/buble/bin/buble/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/buble/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules/buble/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../buble/bin/buble" "$@"
else 
  exec node  "$basedir/../buble/bin/buble" "$@"
fi
