#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/rollup@2.11.2/node_modules/rollup/dist/bin/rollup/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/rollup@2.11.2/node_modules/rollup/dist/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/rollup@2.11.2/node_modules/rollup/dist/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/rollup@2.11.2/node_modules/rollup/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/rollup@2.11.2/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/.pnpm/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules:/Users/Panoply/Sites/rollup-plugins/node_modules:/Users/Panoply/Sites/node_modules:/Users/Panoply/node_modules:/Users/node_modules:/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/rollup/dist/bin/rollup/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/rollup/dist/bin/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/rollup/dist/node_modules:/Users/Panoply/Sites/rollup-plugins/jsonc/node_modules/rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
